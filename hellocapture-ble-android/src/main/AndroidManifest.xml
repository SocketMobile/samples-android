<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.socketmobile.android.hellocapture"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.BLUETOOTH_CONNECT" />
    <uses-permission android:name="android.permission.BLUETOOTH" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.socketmobile.android.hellocaptureble.MainActivity"
            android:screenOrientation="portrait"
            android:taskAffinity=""
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.socketmobile.android.hellocaptureble.NonCaptureActivity"
            android:taskAffinity="" />
        <activity
            android:name="com.example.socketmobile.android.hellocaptureble.SecondCaptureActivity"
            android:taskAffinity="" />

        <meta-data
            android:name="com.socketmobile.capture.APP_KEY"
            android:value="MC0CFCgvClwaKycA4W6FfqAiZzXGGxScAhUA4jOj3SLJHkvdE1lWsQNxPrHBp6U="/>
        <meta-data
            android:name="com.socketmobile.capture.DEVELOPER_ID"
            android:value="BB57D8E1-F911-47BA-B510-693BE162686A"/>
    </application>

</manifest>
